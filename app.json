{
  "expo": {
    "name": "Outgraft",
    "slug": "outgraft",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shankarkanase.outgraft"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "shankarkanase",
    "extra": {
      "eas": {
        "projectId": "2018f647-7b2e-4e0f-93a7-e95c6a5c6469"
      }
    }
  }
}
